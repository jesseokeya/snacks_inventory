{
  "name": "snacks_inventory",
  "version": "1.0.0",
  "description": "A simple inventory application for healthy snacks",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rvf build && babel src -d build",
    "start": "nodemon src/server.js --exec babel-node --presets babel-preset-env"
  },
  "keywords": [
    "graphql",
    "express",
    "apollo",
    "morgan"
  ],
  "private": true,
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "minify-simplify",
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "author": "Jesse Okeya",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.3",
    "babel-plugin-minify-simplify": "^0.4.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "morgan": "^1.9.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "gravatar": "^1.6.0",
    "mongoose": "^5.2.3",
    "winston": "^3.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesseokeya/snacks_inventory.git"
  },
  "bugs": {
    "url": "https://github.com/jesseokeya/snacks_inventory/issues"
  },
  "homepage": "https://github.com/jesseokeya/snacks_inventory#readme"
}
